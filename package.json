    "@storybook/preview-api": "^7.4.6",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@tanstack/react-table": "^8.10.1",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "formik": "^2.4.5",
    "i18next": "^23.5.1",
    "i18next-http-backend": "^2.2.2",
