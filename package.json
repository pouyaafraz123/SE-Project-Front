    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.3.1",
    "prettier": "3.0.3",
    "sass": "^1.68.0",
    "storybook": "^7.4.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write --ignore-unknown"
  },
  "msw": {
    "workerDirectory": "public"
